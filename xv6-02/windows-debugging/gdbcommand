set disassembly-flavor intel

define zstartw
  source /workspaces/xv6-02-27/xv6-02/windows-debugging/load-symbols
	directory /workspaces/xv6-02-27/xv6-02/windows-debugging
	set substitute-path /mnt/c C:
	set substitute-path /mnt/d D:
	set substitute-path /mnt/e E:
	load-symbols $eip "/workspaces/xv6-02-27/xv6-02/kernel"
	set *(char *)&gdb_attached = 1
end
